{
  "name": "neomono",
  "displayName": "Neomono",
  "description": "A vibrant, futuristic dark theme with neon accents for modern developers.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Themes"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:neomono.enableNeonDreams",
    "onCommand:neomono.disableNeonDreams"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Neomono",
        "uiTheme": "vs-dark",
        "path": "./themes/Neomono-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "neomono.enableNeonDreams",
        "title": "Neomono: Enable Neon Dreams"
      },
      {
        "command": "neomono.disableNeonDreams",
        "title": "Neomono: Disable Neon Dreams"
      }
    ]
  }
}
